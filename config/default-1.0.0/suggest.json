{
  "rivers": [
    {
      "name": "vip_product_suggest_task",
      "notification": {
        "type": "elasticsearch_regularly_scan",
        "crontab": {
        },
        "host": "{vip_es_host}",
        "filter": {
          "type": "es_regex",
          "union_operator": "and",
          "conditions": [
            {
              "operator": "is",
              "type": "regex",
              "field": "index",
              "expression": "^qmshop-[\\d\\D]*"
            },
            {
              "operator": "is",
              "type": "regex",
              "field": "type",
              "expression": "^Product$"
            },
            {
              "operator": "not",
              "type": "regex",
              "field": "type",
              "expression": "^qmshop-a000000-[\\d\\D]$"
            }
          ]
        }
      },
      "source": {
        "type": "iterator_es_get",
        "size": 200,
        "data_parser": {
          "type": "map",
          "keyword_filter_regex": "^[一-龥A-Za-z0-9]{2,}$",
          "fields": {
            "title": "title",
            "brand": "brand"
          }
        },
        "param_parser": {
          "type": "regex",
          "fields": {
            "adminId": {
              "field": "index",
              "type": "regex",
              "expression": "^qmshop-(?P<adminId>[\\d\\D]+?)-"
            }
          }
        },
        "tags": {
          "default": []
        }
      },
      "processing": {
        "type": "basic_processing",
        "output": {
          "weight": {
            "type": "hits"
          },
          "payloads": {
            "type": "map",
            "fields": {
              "source_type": "source_type",
              "hits": "hits"
            }
          },
          "common_fields": {
            "type": "map",
            "fields": {
              "source_type": "source_type",
              "id": "id"
            }
          }
        }
      },
      "destination": [
        {
          "destination_type": "elasticsearch_processed",
          "reference": "suggest_vip",
          "operation": "create",
          "clear_policy": "every_msg,auto_term"
        }
      ]
    },
